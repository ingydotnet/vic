PIC P16F690;

config variable bits = 16;
config $var5 bits = 8;
config $var6 bits = 32;
config $var7 bits = 64;

Main {
    digital_output PORTC;
    $var1 = 12345;
    $var2 = 67890;
    $var6 = 0;
    $var3 = 0;
    $var6 = 67890;
    $var7 = 0xCAFEFACEDEADBEEF;
    ++$var1;
    --$var2;
    $var3 = $var1 + $var2;
    $var3 = $var2 - $var1;
    $var1 = 128;
    $var2 = 32;
    $var4 = $var1 * $var2;
    $var4 = $var4 / $var2;
    $var3 = $var4 % 5;
    # sqrt is a modifier
#    $var3 = sqrt $var4;
    $var5 = $var3;
    --$var7;
    $var7 = $var3;
    --$var5;
    $var5 = ($var1 + (($var3 * ($var4 + $var7) + 5) + $var2));
    $var6 = $var5 + !$var3;
    if $var6 == 1, {
        $var6++;
    }, if $var6 == 2, {
        ++$var6;
    }, {
        $var6 = 0;
    };
}
