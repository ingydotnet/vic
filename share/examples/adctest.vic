PIC P16F690;

pragma adc right_justify = 0;
Main {
    digital_output RC0;
    analog_input RA0;
    # adc_setup clock, channel
    adc_enable 500kHz, AN0;
    Loop {
        adc_read $display;
        delay_ms $display;
        write RC0, 1;
        delay_ms $display;
        write RC0, 0;
        delay 100us;
    }
}
