PIC p16f690;

Main {
    $led7 = table [ 0x3F, 0x06, 0x5B, 0x4F, 0x66, 0x6D,
              0x7C, 0x07, 0x7F, 0x67, 0x77, 0x7C, #FIXME
              0x58, 0x5E, 0x79, 0x71 ];
    $digit = 0;
    digital_output PORTA;
    digital_output PORTB;
    write PORTA, 0;
    Loop {
    #    write PORTB, $led7[$digit];
        $digit++;
        $digit &= 0x0F;
    }
}
