PIC P16F690;

config variable bits = 16;
config $var5 bits = 8;

Main {
    digital_output PORTC;
    $var1 = 12345;
    $var2 = 67890;
    $var1++;
    $var2--;
    $var3 = $var1 + $var2;
    $var3 = $var2 - $var1;
    $var1 = 128;
    $var2 = 32;
    $var4 = $var1 * $var2;
    $var4 = $var4 / $var2;
    $var3 = $var4 % 5;
    # sqrt is a modifier
    $var3 = sqrt $var4;
    $var5 = $var3;
}
